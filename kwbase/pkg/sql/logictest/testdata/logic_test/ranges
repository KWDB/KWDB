query T
SELECT feature_name FROM kwdb_internal.feature_usage WHERE feature_name='sql.show.ranges' AND usage_count > 0
----
sql.show.ranges

subtest range_for_row

statement ok
CREATE TABLE simple_range_for_row(x INT PRIMARY KEY)

statement ok
